    "compress-projects": "cd ../dnas/projects && dna-util -c projects.dna.workdir",
    "test": "RUST_LOG=error TRYORAMA_LOG_LEVEL=info RUST_BACKTRACE=1 TRYORAMA_HOLOCHAIN_PATH=\"holochain\" ts-node src/index.ts"
  },
